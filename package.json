{
  "name": "ticker",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-syntax-jsx": "^6.13.0",
    "babel-plugin-transform-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-merge-sibling-variables": "^0.0.2",
    "babel-plugin-transform-node-env-inline": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-xvdom": "peterwmwong/babel-plugin-xvdom#master",
    "babel-preset-es2015": "^6.16.0",
    "css-loader": "^0.25",
    "eslint": "^3.7.0",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "highlight.js": "^9.7.0",
    "html-webpack-plugin": "^1.7.0",
    "marked": "^0.3.6",
    "octicons": "^4.4.0",
    "style-loader": "^0.13.1",
    "uglify-js": "^2.7.3",
    "vulcanize": "^1.14.7",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "xvdom": "peterwmwong/xvdom#master"
  },
  "scripts": {
    "lint":
      "eslint src/ serviceWorker.js",

    "build:marked":
      "uglifyjs --compress --mangle -- node_modules/marked/lib/marked.js > vendor/marked/marked.min.js",

    "build:highlightjs":
      "build/build-hljs-languages.sh",

    "server":
      "NODE_ENV=development webpack-dev-server --config webpack-server.config.js",

    "build:prod":
      "NODE_ENV=production webpack --config webpack-production.config.js; mv dist/index.html dist/index-pre.html; vulcanize --inline-scripts --inline-css dist/index-pre.html | sed 's/<body>//g' | sed 's/<\\/body>//g' > dist/index.html; ls -al dist/index.html; rm dist/index-pre.html"
  },
  "author": "peter.wm.wong@gmail.com",
  "license": "ISC"
}
